{
  "name": "@nxus/renderer",
  "version": "2.3.1",
  "description": "Rendering framework for Nxus applications/",
  "main": "lib/index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --compilers js:babel/register -R spec",
    "compile": "babel --optional runtime -d lib/ src/",
    "prepublish": "npm run compile",
    "postpublish": "npm run docs-readme && npm run commit-update-docs",
    "docs-readme": "export NAME=`npm view . name`; export VERSION=`npm view . version`; documentation readme ./src/*.js --name $NAME --project-version $VERSION --readme-file ./README.md -s $NAME",
    "commit-update-docs": "git add ./README.md && git commit -m 'Updated README API Docs' && git push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nxus/renderer.git"
  },
  "keywords": [
    "nxus",
    "rendering",
    "ejs",
    "handlebars",
    "liquid"
  ],
  "author": "mjreich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nxus/renderer/issues"
  },
  "homepage": "https://github.com/nxus/renderer",
  "dependencies": {
    "babel-runtime": "^5.7.0",
    "bluebird": "^3.0.6",
    "ejs": "^2.3.4",
    "moment": "^2.11.2",
    "underscore": "^1.8.3"
  },
  "peerDependencies": {
    "@nxus/core": "^2.3.0"
  },
  "devDependencies": {
    "@nxus/core": "^2.3.0",
    "babel": "^5.6.23",
    "mocha": "^2.2.5",
    "should": "^7.0.2",
    "documentation": "^4.0.0-beta"
  }
}
